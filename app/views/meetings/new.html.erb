<h1>Create a meeting</h1>

<%= form_tag "/meetings", method: :post do %>
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>
  <div>
    <%= label_tag :address %>
    <%= text_field_tag :address %>
  </div>
  <div>
    <%= label_tag :start_time %>
    <%= datetime_local_field_tag :start_time %>
  </div>
  <div>
    <%= label_tag :end_time %>
    <%= datetime_local_field_tag :end_time %>
  </div>
  <div>
    <%= label_tag :notes %>
    <%= text_area_tag :notes %>
  </div>
  <div>
    <%= submit_tag %>
  </div>
<% end %>
